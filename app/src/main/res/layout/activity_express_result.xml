<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="#F29F80"
    tools:context=".view.activity.ExpressResultActivity">
    <jcc.mycustomtopview.MyCustomTopView
        android:layout_width="match_parent"
        android:layout_height="48dp"
        app:leftImgView="@drawable/aditop"
        app:centerTextTitle="查询结果"
        android:paddingTop="12dp"
        android:paddingBottom="12dp"
        android:id="@+id/expressresult_TopView"/>
    <ImageView
        android:layout_below="@id/expressresult_TopView"
        android:id="@+id/image_expressresult"
        android:layout_width="36dp"
        android:layout_height="36dp"
        android:layout_marginTop="20dp" />

    <TextView
        android:id="@+id/tv_expressresult"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/image_expressresult"
        android:layout_marginLeft="150dp"
        android:layout_marginTop="20dp"
        android:textSize="20sp" />

    <ListView
        android:id="@+id/listview_expressresult"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@id/tv_expressresult"
        android:layout_marginTop="20dp" />
</RelativeLayout>
